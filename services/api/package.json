{
  "name": "@fundi-wangu/api",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up --migrations-dir migrations",
    "migrate:down": "node-pg-migrate down --migrations-dir migrations",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fundi-wangu/shared-types": "*",
    "@fundi-wangu/utils": "*",
    "@fundi-wangu/i18n-strings": "*",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "node-pg-migrate": "^7.0.0",
    "pg": "^8.11.0",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "sharp": "^0.33.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  }
}
